#include<stdio.h>
#define  MAXSIZE  20
int main()
{
	typedef struct
{
	int data[MAXSIZE];
	int len;
}SqList;
	SqList A={1,3,5,7};
	SqList B={2,6,8,9};
	SqList C;
	int i=0;
	int	j=0;
	int	k=0;
	
	A.len=4;
	B.len=4;
	while(i<=A.len && j<=B.len)
	{
		if(A.data[i]<=B.data[j])
		{
			C.data[k]=A.data[i];
			i++;
			k++;
		}
		else
		{
			C.data[k]=B.data[j];
			j++;
			k++;
		}
	}
		k--;

	while(i<A.len)
		{
			C.data[k]=A.data[i];
			k++;
			i++;
		}
	while(j<B.len)
		{
			C.data[k]=B.data[j];
			k++;
			j++;
		}

	C.len=k;
	for(i=0;i<A.len;i++)
	{
		printf("A.data[%d]=%d ",i,A.data[i]);
	}
	printf("\n\n");
	for(i=0;i<B.len;i++)
	{
		printf("B.data[%d]=%d ",i,B.data[i]);
	}
	printf("\n\n");
	for(i=0;i<k;i++)
	{
		printf("C.data[%d]=%d ",i,C.data[i]);
	}
	return 0;
}
